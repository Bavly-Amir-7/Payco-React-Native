{
  "expo": {
    "name": "PaycoApp",
    "slug": "PaycoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/payco.png",
    "scheme": "payco",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false, 
    "jsEngine": "jsc", 

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/payco.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/coffee-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da93a001-651c-4d58-beec-0647c4504745"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/da93a001-651c-4d58-beec-0647c4504745"
    },
    "owner": "bavly707",
    "assetBundlePatterns": ["**/*"], 
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "json", "cjs"]
    },
    "permissions": [
      "CAMERA",
      "LOCATION",
      "NOTIFICATIONS"
    ],
    "debuggerHost": "localhost:19000"
  }
}
